{
  "manifestVersion": 1,
  "id": "FeaturePercentage-widget",
  "version": "1.0.4",
  "name": "Feature Percentage Complete",
  "publisher": "MattEVisuals",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.work"
  ],
  "description": "Show the percentage complete by story count of Features",
  "icons": {
    "default": "app/Images/logo.png"
  },
  "categories": [
    "Plan and track"
  ],
  "tags": [
    "agile",
    "reporting",
    "dashboard",
    "widget"
  ],

  "links": {
    "home": {
      "uri": "https://marketplace.visualstudio.com/items?itemName=MattEVisuals.FeaturePercentage-widget"
    },
    "support": {
      "uri": "https://www.fabrikam-fiber-inc.com/support"
    },
    "getstarted": {
      "uri": "https://www.fabrikam-fiber-inc.com/help/getstarted"
    },
    "learn": {
      "uri": "https://www.fabrikam-fiber-inc.com/features"
    },
    "repository": {
      "uri": "https://github.com/fabrikam-fiber-inc/tools"
    },
    "issues": {
      "uri": "https://github.com/fabrikam-fiber-inc/tools/issues"
    }
  },

  "screenshots": [
    {
      "path": "assets/Screenshot.png"
    }
  ],
  "branding": {
    "color": "rgb(1, 122, 215)",
    "theme": "dark"
  },

  "contributions": [
    {
      "id": "FeaturePercentage-widget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog"
      ],

      "properties": {
        "name": "Feature Percentage Complete",
        "description": "A widget to display a list of Features and each one's percentage complete",
        "catalogIconUrl:": "app/Images/logo.png",
        "previewImageUrl": "app/Images/preview.png",
        "uri": "app/dashboard-widget.html",

        "supportedSizes": [
          {
            "rowSpan": 2,
            "columnSpan": 3
          },
          {
            "rowSpan": 1,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 2
          },
          {
            "rowSpan": 1,
            "columnSpan": 1
          },
          {
            "rowSpan": 4,
            "columnSpan": 4
          }
        ],

        "supportedScopes": [ "project_team" ]
      }
    }
  ],

  "files": [
    {
      "path": "app/dashboard-widget.html",
      "addressable": true
    },
    {
      "path": "app/Content",
      "addressable": true
    },
    {
      "path": "app/Images",
      "addressable": true
    },
    {
      "path": "app/Scripts",
      "addressable": true
    }
  ],

  "content": {
    "details": {
      "path": "assets/Overview.md"
    },
    "license": {
      "path": "assets/Licence.md"
    }
  }

}
